{
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2020": true
  },
  "globals": { // エラーにしたくないグローバル変数がある場合
    //"$": true,
    //"jQuery": true
  },
  "rules": {
    "prettier/prettier": ["error", {}]
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "prettier"]
}
